{
  "manifest_version": 2,

  "name": "Distract Me",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "1.1",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
   "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
]
}